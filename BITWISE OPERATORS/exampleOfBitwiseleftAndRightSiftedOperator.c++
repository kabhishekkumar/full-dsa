// Bitwise left sifted and right sifted operator

#include<iostream>
using namespace std; 
int main(){
    // left sifted operator
    cout<<(4<<1)<<endl;
    cout<<(10<<2)<<endl;
    cout<<(15<<4)<<endl;
    cout<<(9<<1)<<endl;
    // right sifted operator

     cout<<(20>>1)<<endl;
    cout<<(10>>1)<<endl;  
    cout<<(5>>2)<<endl;
    cout<<(10>>2)<<endl;

}